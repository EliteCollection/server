<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.team.manage.dao.Liquibase0011Mapper">
    <resultMap id="baseMap" type="com.team.manage.entity.Liquibase0011">
        <id column="id"  property="id" />
        <result column="firstname" property="firstname" />
        <result column="lastname"  property="lastname" />

    </resultMap>

    <select id="getLiquibase0011" resultMap="baseMap">
        select  * from  liquibase0011
    </select>

    <insert id="insertValues" parameterType="com.team.manage.entity.Liquibase0011">
        insert into liquibase0011 values (#{id}, #{firstname}, #{lastname})
    </insert>
</mapper>